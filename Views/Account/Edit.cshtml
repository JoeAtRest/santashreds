                    <td>@Model.GetLocation("FIN").TrickComplete</td>
                    <td>@Model.GetLocation("FIN").CodeEnteredDate</td>
                    <td>Edit</td>
                    </tr>
                }   
                else
                {                   
                    <tr>
                    <td>FIN</td>
                    <td></td>
                    <td></td>
                    <td>Edit</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>